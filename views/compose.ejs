
<%- include("partials/header"); -%>
    <form class="" action="/compose" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" required type="text" name="postTitle">
        <label>Post</label>
        <textarea class="form-control textarea" name="postBody" rows="5" cols="30"></textarea>
      </div>
      <a href="/" class="btn btn-outline-primary">Cancel</a>
      <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </form>
<%- include("partials/footer"); -%>
